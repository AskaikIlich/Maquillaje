CREATE TABLE "PERSONA" (
  "ID_persona" INT,
  "nombre" VARCHAR(30),
  "apellido" VARCHAR(30),
  "cedula" INT,
  "telefono" VARCHAR(15),
  "correo" VARCHAR(30),
  PRIMARY KEY ("ID_persona")
);

CREATE TABLE "STATUS_TAREA" (
  "ID_statusTarea" INT,
  "statusTarea" VARCHAR(30),
  PRIMARY KEY ("ID_statusTarea")
);

CREATE TABLE "TAREAS_ASIGNADAS" (
  "ID_tareasAsigna" INT,
  "fechaInicio" DATE,
  "fechaFinal" DATE,
  "descripcionTarea" char,
  "observacion" VARCHAR(100),
  "FK_tarea" int,
  "FK_statusTarea" int,
  "FK_evaluacion" Tipo,
  PRIMARY KEY ("ID_tareasAsigna")
);

CREATE TABLE "CHAMBISTA" (
  "ID_chambista" INT,
  "FK_persona" INT,
  "FK_division" INT,
  "fechaIngreso" DATE,
  "fechaCulminacion" DATE,
  PRIMARY KEY ("ID_chambista")
);

CREATE TABLE "CHAMBISTA_TAREA" (
  "ID_chamTarea" INT,
  "FK_chambista" int,
  "FK_tarea" int,
  "observacion" varchar(50),
  PRIMARY KEY ("ID_chamTarea"),
  CONSTRAINT "FK_CHAMBISTA_TAREA.FK_tarea"
    FOREIGN KEY ("FK_tarea")
      REFERENCES "TAREAS_ASIGNADAS"("ID_tareasAsigna"),
  CONSTRAINT "FK_CHAMBISTA_TAREA.FK_chambista"
    FOREIGN KEY ("FK_chambista")
      REFERENCES "CHAMBISTA"("ID_chambista")
);

CREATE TABLE "DETALLES_CITA" (
  "ID_cita" INT,
  "fechaCita" DATE,
  "horaCita" TIME,
  "FK_tipoCita" INT,
  "FK_encargado" int,
  "FK_pauta" int,
  PRIMARY KEY ("ID_cita")
);

CREATE TABLE "ANCLA" (
  "ID_ancla" INT,
  "FK_empleado" INT,
  PRIMARY KEY ("ID_ancla")
);

CREATE TABLE "EMPLEADO" (
  "ID_empleado" INT,
  "FK_persona" INT,
  "codTrabajador" INT,
  "FK_division" INT,
  "FK_cargo" INT,
  PRIMARY KEY ("ID_empleado")
);

CREATE TABLE "ENTREGADO_LAVANDERIA" (
  "ID_entregadoLavanderia" int,
  "cantEntregada" int,
  "FK_responsable" int,
  "FK_insumoLavanderia" int,
  "fechaEntrega" DATE,
  "fechaDevolucion" DATE,
  "observacion" char,
  PRIMARY KEY ("ID_entregadoLavanderia"),
  CONSTRAINT "FK_ENTREGADO_LAVANDERIA.FK_responsable"
    FOREIGN KEY ("FK_responsable")
      REFERENCES "EMPLEADO"("ID_empleado")
);

CREATE TABLE "ARTICULO" (
  "ID_articulo" INT,
  "FK_descripcion" INT,
  "FK_color" int,
  "FK_detalle" int,
  "FK_estatusArt" int,
  "FK_marca" int,
  PRIMARY KEY ("ID_articulo")
);

CREATE TABLE "ARTICULOxLAVANDERIA" (
  "ID_articuloRegistro" INT,
  "FK_articulo" int,
  "FK_entregadoLavan" int,
  PRIMARY KEY ("ID_articuloRegistro"),
  CONSTRAINT "FK_ARTICULOxLAVANDERIA.FK_entregadoLavan"
    FOREIGN KEY ("FK_entregadoLavan")
      REFERENCES "ENTREGADO_LAVANDERIA"("ID_entregadoLavanderia"),
  CONSTRAINT "FK_ARTICULOxLAVANDERIA.FK_articulo"
    FOREIGN KEY ("FK_articulo")
      REFERENCES "ARTICULO"("ID_articulo")
);

CREATE TABLE "PROGRAMACION" (
  "ID_PROGRAMACION" INT,
  "FK_programa" int,
  "horaPrograma" TIME,
  "fechaPrograma" DATE,
  PRIMARY KEY ("ID_PROGRAMACION")
);

CREATE TABLE "PAUTA" (
  "ID_pauta" INT,
  "FK_programacion" int,
  "FK_presentador" int,
  "FK_ancla" int,
  PRIMARY KEY ("ID_pauta"),
  CONSTRAINT "FK_PAUTA.FK_programacion"
    FOREIGN KEY ("FK_programacion")
      REFERENCES "PROGRAMACION"("ID_PROGRAMACION")
);

CREATE TABLE "TIPO_US" (
  "ID_tipoUs" INT,
  "tipoUsuario" VARCHAR(30),
  PRIMARY KEY ("ID_tipoUs")
);

CREATE TABLE "INVITADO" (
  "ID_invitado" int,
  "FK_persona" INT,
  PRIMARY KEY ("ID_invitado")
);

CREATE TABLE "INSUMOS_LAVANDERIA" (
  "ID_insumoLavanderia" int,
  "articuloLimpieza" vachar(20),
  "cantExist" int,
  PRIMARY KEY ("ID_insumoLavanderia"),
  CONSTRAINT "FK_INSUMOS_LAVANDERIA.ID_insumoLavanderia"
    FOREIGN KEY ("ID_insumoLavanderia")
      REFERENCES "ENTREGADO_LAVANDERIA"("FK_insumoLavanderia")
);

CREATE TABLE "STATUS_US" (
  "ID_statusUs" INT,
  "statusUsuario" VARCHAR(30),
  PRIMARY KEY ("ID_statusUs")
);

CREATE TABLE "DETALLE_PRESTAMO" (
  "ID_prestamo" Tipo,
  "fechaRecibido" DATE,
  "FK_estatusRegistro" int,
  "FK_detallesCita" int,
  PRIMARY KEY ("ID_prestamo")
);

CREATE TABLE "ARTICULOxREGISTRO" (
  "ID_articuloRegistro" INT,
  "FK_articulo" int,
  "FK_prestamo" int,
  "entregado" bool,
  "observacion" char,
  PRIMARY KEY ("ID_articuloRegistro"),
  CONSTRAINT "FK_ARTICULOxREGISTRO.FK_articulo"
    FOREIGN KEY ("FK_articulo")
      REFERENCES "ARTICULO"("ID_articulo"),
  CONSTRAINT "FK_ARTICULOxREGISTRO.FK_prestamo"
    FOREIGN KEY ("FK_prestamo")
      REFERENCES "DETALLE_PRESTAMO"("ID_prestamo")
);

CREATE TABLE "USUARIO" (
  "ID_usuario" INT,
  "usuario" VARCHAR(30),
  "clave" INT,
  "FK_persona" INT,
  "FK_status" INT,
  "FK_tipoUs" INT,
  PRIMARY KEY ("ID_usuario")
);

CREATE TABLE "EVALUACION" (
  "ID_evaluacionTarea" INT,
  "letra" VARCHAR(1),
  "significado" VARCHAR(30),
  PRIMARY KEY ("ID_evaluacionTarea")
);

